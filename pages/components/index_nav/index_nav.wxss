.zdy_nav{
  background: linear-gradient(#0477ff,#1284ff);
  position: fixed;
  width: 100%;
  z-index: 9999999;
  top: 0;
}
.navbar{
  position: relative;
}
.back-icon, .home-icon{
  max-width: 15px;
  height: 100%;
  position: absolute;
  transform: translateY(-50%);
  top: 50%;
  display: flex;
}
.back-icon{
  left: 16px;
}
.home-icon{
  left: 44px
}
.back-icon image{
  width: 28px;
  height: 28px;
  margin: auto;
}
.home-icon image{
  width: 20px;
  height: 20px;
  margin: auto;
}
.nav-title, .nav-icon{
  position: absolute;
  transform: translate(-50%, -50%);
  left: 50%;
  top: 50%;
  font-size: 0;
  font-weight: bold;
}