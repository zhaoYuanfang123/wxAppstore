/* pages/addressManagement/addressManagement.wxss */
page{
  background: #f5f5f5;
}
.listBox{
  box-sizing: border-box;
  padding-bottom: 100rpx;
}
.listItem{
  margin-bottom: 20rpx;
  padding: 40rpx 20rpx;
  background: #fff;
}
.listItem .p1{
  color: #222;
  font-size: 34rpx;
}
.listItem .p1 text:nth-child(2){
  margin-left: 20rpx;
}
.listItem .address{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 20rpx;
}
.listItem .address image{
  max-width: 20rpx;
  flex: 1;
}
.listItem .address .lb{
  display: flex;
  flex: 3;
}
.listItem .address .lb .default{
  color: #ffc44c;
  width: 170rpx;
}
.listItem .address .lb .addressInfo{
  color: #999;
  width: 100%;
}
.listItem .address .rb{
  width: 100rpx;
  text-align: right;
}
.footer{
  background: #1890ff;
  color: #fff;
  text-align: center;width: 100%;
  position: fixed;
  bottom: 0;
  padding: 30rpx 0;
}